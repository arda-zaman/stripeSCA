{
  "presets": [
    "babel-preset-env",
    "stage-1",
    "babel-preset-react"
  ],
  "plugins": [
    "transform-class-properties",
    [
      "transform-es2015-modules-commonjs",
      {
        "allowTopLevelThis": true
      }
    ],
    "transform-react-inline-elements",
    "transform-react-constant-elements",
    [
      "add-module-exports",
      {
        "addDefaultProperty": true
      }
    ]
  ]
}